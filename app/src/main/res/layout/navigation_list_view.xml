<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:id="@+id/activity_notes_nv_notes"
    style="@style/NvButton"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout
        style="@style/NvButtonContainer"
        tools:ignore="UseCompoundDrawables">
        <ImageView
            style="@style/NvButtonIcon"
            android:src="@drawable/ic_hashtag"
            android:contentDescription="@string/navigation_view_hashtags"
            app:tint="@color/nv_button_text" />
        <TextView
            android:id="@+id/navigation_list_view_name"
            style="@style/NvButtonText"/>
    </LinearLayout>
</FrameLayout>